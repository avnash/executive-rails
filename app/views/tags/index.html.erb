<% content_for :title do%>
  Home
<% end %>

<% if signed_in? %>

  	<%= render "shared/sidebar" %>
	<br>
<div class="post_main_container">
	<% @posts.each do |post| %>
  		<%= render post %>
	<% end %>

	<% if @posts.length>10 %>
    	<%= render 'shared/loadmore' %>
	<% end %>
</div>
<% else %>

  	<%= render 'shared/unsigned' %>

<% end %>
